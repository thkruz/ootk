{
  "name": "sgp4-js",
  "version": "0.4.0",
  "description": "SGP4/SDP4 Calculation Library",
  "main": "src/sgp4.js",
  "scripts": {
    "build": "node node_modules/webpack/bin/webpack.js --config ./scripts/webpack.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:test": "eslint test",
    "test": "jest -c scripts/jest-std.json",
    "test:coverage": "jest --coverage -c scripts/jest-std.json",
    "test:performance": "node test/performance/timing.test.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thkruz/satjs"
  },
  "keywords": [
    "sgp4",
    "satellite"
  ],
  "author": "Theodore Kruczek <theodore.kruczek@gmail.com> (https://github.com/thkruz/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thkruz/satjs/issues"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "ignore",
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "off"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.11.5",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.9.0",
    "html-loader": "^1.3.2",
    "jest": "^26.4.2",
    "terser-webpack-plugin": "^5.1.1",
    "threads-plugin": "^1.4.0",
    "typescript-eslint": "^0.0.1-alpha.0",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  },
  "homepage": "https://github.com/thkruz/sgp4-js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {
    "threads": "^1.6.3",
    "tiny-worker": "^2.3.0"
  }
}
